doctype html
html
  include inc/header
  include inc/scripts

  body
  .navbar.navbar-default.navbar-static-top(role='navigation')
      .container
          .navbar-header
            button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
              span.sr-only Toggle navigation
              span.icon-bar
              span.icon-bar
              span.icon-bar
            a.navbar-brand(href='/') #{brand}
          .navbar-collapse.collapse
            ul.nav.navbar-nav
              - var obj = { 'upload':'Upload Photo', 'myphotos':'My Photos', 'allphotos': 'All Photos', 'account' : 'Account' }
                - each val, key in obj
                  - if (id == key)
                    li.active
                      a(href='#{key}') #{val}
                  - else            
                    li
                      a(href='#{key}') #{val}
    div.container
        div.jumbotron

            block content
  
  include inc/footer
